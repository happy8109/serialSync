{
  "serial": {
    "port": "COM3",
    "baudRate": 115200,
    "dataBits": 8,
    "stopBits": 1,
    "parity": "none",
    "timeout": 5000,
    "autoReconnect": true,
    "reconnectInterval": 3000,
    "maxReconnectAttempts": 5
  },
  "logging": {
    "level": "info",
    "file": "./logs/sync.log",
    "maxSize": "10m",
    "maxFiles": 5,
    "format": "combined"
  },
  "server": {
    "port": 3001,
    "host": "0.0.0.0"
  },
  "sync": {
    "chunkSize": 2048,
    "timeout": 100,
    "retryAttempts": 3,
    "compression": true,
    "autoAccept": true,
    "saveDir": "./received_files",
    "confirmTimeout": 30000
  }
} 